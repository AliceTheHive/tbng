[Unit]
Description=WebUI for TorBoxNG

[Service]
ExecPreStart=/home/zno/tbng/engine/tbng.py mode restore
ExecStart=/usr/bin/nodejs /home/zno/tbng/bin/www
Restart=always
StandardOutput=syslog
StandardError=syslog
SyslogIdentifier=webui-torbox
User=zno
Group=zno
Environment=NODE_ENV=production

[Install]
WantedBy=multi-user.target
